{
  "manifest_version": 2,

  "name": "KA-VR",
  "description": "KA-VR Virtual Assistant",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "browser_action": {
   // "default_icon": "icon.png",
   "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "geolocation"
  ]
}