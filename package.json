{
  "name": "ka-vr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ci": "npm test && npm run dev",
    "extension:build": "webpack --progress --config chrome.webpack.config.js",
    "extension:watch": "webpack --watch --progress --config chrome.webpack.config.js",
    "dev-server": "nodemon --delay 1.5 dev/server/server.js",
    "dev": "npm run lint && (npm run watch-server & npm run dev-server & npm run watch-client)",
    "dev:skip": "bash devskip.sh",
    "test": "mocha test/browser.js test/**/*",
    "postinstall": "npm run build",
    "test:watch": "mocha tests/client/components/* --compilers js:babel-core/register --watch --reporter min",
    "lint": "eslint --quiet src/",
    "start": "node prod/server/server.js",
    "build": "webpack --progress --config webpack-prod.config.js && babel src/server -d prod/server",
    "start:production": "node prod/server/server.js",
    "watch-server": "babel --watch src/server -d dev/server",
    "watch-client": "webpack --watch --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KA-VR/KA-VR.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KA-VR/KA-VR/issues"
  },
  "homepage": "https://github.com/KA-VR/KA-VR#readme",
  "dependencies": {
    "apoc": "^2.1.6",
    "babel-polyfill": "^6.9.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "image-webpack-loader": "^1.8.0",
    "isomorphic-fetch": "^2.2.1",
    "path": "^0.12.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.0",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.8.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.12"
  }
}
